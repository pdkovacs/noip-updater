{
  "name": "noip-updater",
  "version": "1.0.0",
  "description": "Regularly check public IP and set the new IP on NOIP.com if changed",
  "main": "index.js",
  "dependencies": {
    "@types/winston": "^2.3.0",
    "immutable": "^3.8.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^7.0.12"
  },
  "scripts": {
    "build": "tsc",
    "run": "rm -rf dist && tsc && node dist/noip-updater.js",
    "package": "npm run build && tar -C $(dirname $PWD) -czf $(basename $PWD).tgz --exclude 'node_modules/@types' --exclude '.DS_Store' $(basename $PWD)/node_modules $(basename $PWD)/dist"
  },
  "author": "peter.dunay.kovacs@gmail.com",
  "license": "Apache-2.0"
}
